{
  "name": "els-togo-website",
  "version": "1.0.0",
  "description": "## VHosts",
  "main": "webpack.mix.js",
  "scripts": {
    "dev": "mix",
    "watch": "mix watch",
    "prod": "mix --production",
    "dev:scss": "npm run dev --chore=dev:scss",
    "dev:js": "npm run dev --chore=dev:js",
    "prod:scss": "npm run prod --chore=prod:scss",
    "prod:js": "npm run prod --chore=prod:js",
    "lint:scss": "npm run dev --chore=lint:scss",
    "lint:js": "npm run dev --chore=lint:js",
    "lint:php": "npm run dev --chore=lint:php",
    "prettier:scss": "npm run dev --chore=prettier:scss",
    "prettier:js": "npm run dev --chore=prettier:js",
    "optimize:picture": "npm run dev --chore=optimize:picture",
    "version": "npm run dev --chore=version",
    "clean": "npm run dev --chore=clean",
    "copy": "npm run dev --chore=copy",
    "build": "npm run prod --chore=all --state=build && npm run dev --state=build"
  },
  "devDependencies": {
    "browser-sync": "^2.29.3",
    "browser-sync-webpack-plugin": "^2.3.0",
    "browserslist": "^4.22.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "laravel-mix": "^6.0.49",
    "laravel-mix-replace-in-file": "^0.1.0",
    "laravel-mix-webp": "^0.1.3",
    "node-css-mqpacker": "^9.0.0",
    "prettier": "^3.2.5",
    "replace-in-file": "^7.1.0",
    "sass": "^1.70.0",
    "sass-loader": "^14.1.0",
    "stylelint": "^16.2.1",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-config-sass-guidelines": "^11.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "webpack-shell-plugin-next": "^2.3.1"
  },
  "keywords": [],
